{
    "version": 2,
    "builds": [
        {
            "src": "setting/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/staticfiles/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "setting/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task",
        "DJANGO_SETTINGS_MODULE": "setting.settings",
        "VERCEL_ENV": "production"
    }
} 