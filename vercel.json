{
    "version": 2,
    "builds": [
        {
            "src": "setting/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11.9"
            }
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/media/(.*)",
            "dest": "/media/$1"
        },
        {
            "src": "/(.*)",
            "dest": "setting/wsgi.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task",
        "DJANGO_SETTINGS_MODULE": "setting.settings"
    },
    "buildCommand": "bash build_files.sh"
} 